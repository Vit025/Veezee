<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
               xmlns:s="library://ns.adobe.com/flex/spark">
	
	<fx:Metadata>
	  [HostComponent("spark.components.Application")]
	</fx:Metadata>
	
	<s:states>
	  <s:State name="normal" />
	  <s:State name="disabled"/>
	</s:states>
	
	<s:Rect top = "0" right = "0" bottom = "0" left = "0">
		<s:fill>
			<s:SolidColor color="0x003846"/>
		</s:fill>
	</s:Rect>
	
	<s:Rect top = "0" right = "0" bottom = "0" left = "0">
		<s:fill>
           <s:LinearGradient rotation="90">
                <s:entries>
                    <s:GradientEntry color="0x000000" ratio="0.00" alpha="1"/>
                    <s:GradientEntry color="0x000000" ratio="0.5" alpha="0.2"/>
                    <s:GradientEntry color="0x000000" ratio="1" alpha="1"/>
                </s:entries>
            </s:LinearGradient>
		</s:fill>
	</s:Rect>
	
	<s:Group id="contentGroup" top = "0" right = "0" bottom = "0" left = "0"/>
	
</s:Skin>